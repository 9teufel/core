        <target>Betaalmethoden</target>
      </trans-unit>
      <trans-unit id="tl_iso_payment.opp_password.0">
        <source>Password</source>
        <target>Wachtwoord</target>
      </trans-unit>
      <trans-unit id="tl_iso_payment.groups.0">
        <source>Member groups</source>
        <target>Ledengroepen</target>
      </trans-unit>
      <trans-unit id="tl_iso_payment.groups.1">
        <source>Restrict this payment method to certain member groups.</source>
        <target>Beperk deze betaalmethode tot bepaalde ledengroepen.</target>
      </trans-unit>
      <trans-unit id="tl_iso_payment.guests.0">
        <source>Show to guests only</source>
        <target>Alleen zichtbaar voor gasten</target>
      </trans-unit>
      <trans-unit id="tl_iso_payment.enabled.0">
        <source>Enabled</source>
        <target>Ingeschakeld</target>
      </trans-unit>
